<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPaneChangeUser" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxlogin.AccountController">
   <children>
      <GridPane layoutX="165.0" layoutY="154.0" prefHeight="91.0" prefWidth="269.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnChangeUser" mnemonicParsing="false" onAction="#onButtonChangeUser" text="Change Username" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="btnCancelUser" mnemonicParsing="false" onAction="#onButtonCancelUser" prefHeight="26.0" prefWidth="124.0" text="Cancel" GridPane.rowIndex="2" />
            <Label text="New Username" />
            <Label text="Password " GridPane.rowIndex="1" />
            <PasswordField fx:id="userChangePass" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="newUser" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
